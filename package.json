{
  "name": "mocks-react",
  "version": "0.1.0",
  "description": "mocks-react",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://us-npm.pkg.dev/truepay-tools/npm/"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "auth": "npx google-artifactregistry-auth --repo-config=.npmrc --credential-config=.npmrc",
    "preinstall": "pnpm run auth",
    "prepublishOnly": "clean-package",
    "postpublish": "clean-package restore",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "start": "storybook dev -p 9009",
    "dev": "pnpm run build:pkg --watch",
    "storybook:no-cache": "concurrently \"pnpm run storybook --no-manager-cache\"",
    "test": "jest",
    "test:ci": "pnpm run test > coverage.txt",
    "test:integration": "jest -c jest-integ.config.js",
    "test:integration:ci": "pnpm run test:integration > coverage-integration.txt",
    "format": "prettier --write '{src,stories}/**/*.{js,jsx,ts,tsx,css,md,json}' --config .prettierrc.js",
    "lint": "pnpm run lint:check && pnpm run lint:types",
    "lint:check": "eslint src --ext .ts,.tsx --config .eslintrc.js",
    "lint:types": "tsc --noEmit --pretty",
    "build": "pnpm run build:pkg && pnpm run build:sb",
    "build:pkg": "pnpm run pkg:build:types && pnpm run pkg:build:esm && pnpm run pkg:build:cjs",
    "pkg:build:esm": "cross-env BABEL_ENV=esm babel src --root-mode upward --extensions .ts,.tsx -d dist/esm --source-maps --ignore **/__tests__/*",
    "pkg:build:cjs": "cross-env BABEL_ENV=cjs babel src --root-mode upward --extensions .ts,.tsx -d dist --source-maps --ignore **/__tests__/*",
    "pkg:build:types": "vite build --config vite.lib.config.js",
    "build:sb": "storybook build",
    "clean": "rm -rf storybook-static dist",
    "purge": "rm -rf node_modules pnpm-lock.yaml storybook-static dist"
  },
  "keywords": [],
  "author": "robsond.mathias@gmail.com",
  "dependencies": {
    "@types/lodash": "^4.14.199",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "msw": "^1.3.2",
    "styled-components": "^6.1.8"
  },
  "peerDependencies": {
    "@types/lodash": "^4.14.199",
    "lodash": "^4.17.21",
    "msw": "^1.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/runtime": "^7.23.1",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@storybook/addon-docs": "^7.4.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/blocks": "^7.4.6",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.4.6",
    "@storybook/react-vite": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.4.6",
    "@swc/core": "^1.3.91",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.3",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.1.0",
    "clean-package": "^2.2.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.0.2",
    "husky": "^8.0.3",
    "install-peerdeps": "^3.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup-plugin-copy": "^3.5.0",
    "storybook": "^7.4.6",
    "style-dictionary": "^3.8.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.10",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-svgr": "^4.1.0"
  },
  "clean-package": {
    "remove": [
      "scripts"
    ]
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.2.24",
      "@types/react-dom": "18.2.8",
      "@mui/lab": "5.0.0-alpha.146",
      "@mui/material": "5.14.11",
      "@mui/styles": "5.14.11",
      "@mui/system": "5.14.11",
      "@mui/types": "7.2.4",
      "@mui/utils": "5.14.11",
      "react": "18.2.0",
      "react-dom": "18.2.0"
    }
  }
}
